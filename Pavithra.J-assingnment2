import random
while(True):
    temperature=random.randint(10,99)
    humidity=random.randint(10,99)
    print("Curent Temperature is ",temperature)
    print("Curent Humidity is ",humidity,"%")
    temperature_ref=37
    humidity_ref=35
    if temperature>temperature_ref and humidity<humidity_ref:
        print("Alaram is ON")
    else:
        print("Alaram is OFF")
        break;
